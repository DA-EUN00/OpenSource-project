(defun deleteall (a i) (deleteall1 a i NIL))
(defun deleteall1 (a i1 i2)
	(if (null i1)
		i2
	   (if(equal a (car i1))
		   (deleteall1 a (cdr i1) i2)
	     (if(atom (car i1))
		     (deleteall1 a (cdr i1) (append (list(car i1)) i2))
		   (deleteall1 a (cdr i1) (append i2 (deleteall2 a (car i1) NIL))))
            )
          )
        )

(defun deleteall2 (a i1 i2)
	(if(null i1)
		(list i2)
	    (if(equal a (car i1))
		    (deleteall2 a (cdr i1) i2)
		(if(atom (car i1))
			(deleteall2 a (cdr i1) (append (list(car i1)) i2))
		    (deleteall2 a (cdr i1) (append i2 (deleteall a (car i1) NIL))))
		)
	    )
	 )
